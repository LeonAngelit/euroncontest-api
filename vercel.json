{
	"version": 2,
	"buildCommand": "npm install @sparticuz/chromium",
	"functions": {
	  "index.js": {
		"runtime": "nodejs20.x",
		"memory": 1024,
		"maxDuration": 60
	  }
	},
	"routes": [
	  {
		"src": "/(.*)",
		"dest": "index.js",
		"methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
		"headers": {
		  "Access-Control-Allow-Origin": "*"
		}
	  }
	]
  }