{
	"version": 2,
	"buildCommand": "vercel build; node node_modules/puppeteer-core/install.js;",

	"builds": [
		{
			"src": "index.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "index.js",
			"methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
			"headers": {
				"Access-Control-Allow-Origin": "*"
			}
		}
	]
}
